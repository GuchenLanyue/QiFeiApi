<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="5">
	<parameter name="platform" value="release" />
	<parameter name="sourcesDir" value="/sources" />
	<parameter name="CaseList" value="/case/CaseList.xlsx" />
	<parameter name="basePath" value="http://console.t.upvi.com/bapi" />
	
	<test name="获取子部门ID" preserve-order="true">
		<groups>
			<define name="GetOrganizationByName">
				<include name="getOrganizationByName_Test" />
			</define>
		</groups>
		<classes>
			<class name="com.qifei.test.ProcessTest">
				<methods>
					<include name="getOrganizationByName_Test" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="新增员工" preserve-order="true">
		<groups>
			<define name="AddMember">
				<include name="addMember_Test" />
			</define>
		</groups>
		<classes>
			<class name="com.qifei.test.ProcessTest">
				<methods>
					<include name="addMember_Test" />
				</methods>
			</class>
		</classes>
	</test>
	
</suite>