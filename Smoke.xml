<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="1">
	<parameter name="platform" value="pc" />
	<parameter name="sourcesDir" value="\sources" />
	<parameter name="CaseList" value="\case\CaseList.xlsx" />
	<parameter name="basePath" value="http://console.t.upvi.com/bapi" />
	
	<test name="删除子部门" preserve-order="true">
		<groups>
			<define name="DeleteOrganizationByName">
				<include name="DeleteOrganizationByName_Test" />
			</define>
		</groups>
		<classes>
			<class name="com.qifei.test.ProcessTest">
				<methods>
					<include name="DeleteOrganizationByName_Test" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="审批流程冒烟测试" preserve-order="true">
		<groups>
			<define name="Smoke">
				<include name="process_Test" />
			</define>
		</groups>
		<classes>
			<class name="com.qifei.test.ProcessTest">
				<methods>
					<include name="process_Test" />
				</methods>
			</class>
		</classes>
	</test>
</suite>